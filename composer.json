{
    "name": "arryanggaputra/blasting",
    "description": "The Blasting Framework.",
    "keywords": [
        "framework",
        "blasting"
    ],
    "license": "MIT",
    "type": "project",
    "require":
    {
        "filp/whoops": "^2.1",
        "twig/twig": "^2.0",
        "league/route": "^3.0",
        "zendframework/zend-diactoros": "^1.6"
    },
    "autoload":
    {
        "psr-4":
        {
            "Blasting\\": "src/",
            "Systemblast\\": "system/"
        },
        "files": ["bootstrap/helpers.php"]
    },
    "require-dev":
    {
        "phpunit/phpunit": "^6.2"
    },
    "scripts":
    {
        "test": [
            "@phpunit"
        ],
        "phpunit": "php vendor/bin/phpunit tests"
    }
}